{
    "version": 2,
    "builds": [
        {
            "src": "frontend/run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend/run.py",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ],
    "env": {
        "FLASK_APP": "frontend/run.py",
        "FLASK_ENV": "production"
    }
} 